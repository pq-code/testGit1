#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pq/code/react/reactApp/node_modules/eslint-config-prettier/bin/node_modules:/Users/pq/code/react/reactApp/node_modules/eslint-config-prettier/node_modules:/Users/pq/code/react/reactApp/node_modules:/Users/pq/code/react/node_modules:/Users/pq/code/node_modules:/Users/pq/node_modules:/Users/node_modules:/node_modules:/Users/pq/code/react/reactApp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pq/code/react/reactApp/node_modules/eslint-config-prettier/bin/node_modules:/Users/pq/code/react/reactApp/node_modules/eslint-config-prettier/node_modules:/Users/pq/code/react/reactApp/node_modules:/Users/pq/code/react/node_modules:/Users/pq/code/node_modules:/Users/pq/node_modules:/Users/node_modules:/node_modules:/Users/pq/code/react/reactApp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint-config-prettier/bin/cli.js" "$@"
else
  exec node  "$basedir/../eslint-config-prettier/bin/cli.js" "$@"
fi
