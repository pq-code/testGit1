#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pq/code/react/reactApp/node_modules/.pnpm/react-native@0.76.0_@babel+core@7.26.0_@babel+preset-env@7.26.0_@types+react@18.3.12_react@18.3.1/node_modules/react-native/node_modules:/Users/pq/code/react/reactApp/node_modules/.pnpm/react-native@0.76.0_@babel+core@7.26.0_@babel+preset-env@7.26.0_@types+react@18.3.12_react@18.3.1/node_modules:/Users/pq/code/react/reactApp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pq/code/react/reactApp/node_modules/.pnpm/react-native@0.76.0_@babel+core@7.26.0_@babel+preset-env@7.26.0_@types+react@18.3.12_react@18.3.1/node_modules/react-native/node_modules:/Users/pq/code/react/reactApp/node_modules/.pnpm/react-native@0.76.0_@babel+core@7.26.0_@babel+preset-env@7.26.0_@types+react@18.3.12_react@18.3.1/node_modules:/Users/pq/code/react/reactApp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../react-native@0.76.0_@babel+core@7.26.0_@babel+preset-env@7.26.0_@types+react@18.3.12_react@18.3.1/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../react-native@0.76.0_@babel+core@7.26.0_@babel+preset-env@7.26.0_@types+react@18.3.12_react@18.3.1/node_modules/react-native/cli.js" "$@"
fi
